{
    "name": "exp-sass",
    "description": "Sass Plugin for Expansive",
    "version": "0.2.1",
    "keywords": [
        "expansive-plugin",
        "sass"
    ],
    "licenses": "GPL",
    "repository": {
        "type": "git",
        "url": "git://github.com/embedthis/exp-sass.git"
    },
    "scripts": {
        "postcache": {
            "script": "if (!Cmd.locate('sass')) { if (!Cmd.locate('npm')) { throw 'Cannot locate the \"npm\" utility.' } else { run('npm install -g sass')}}"
        }
    },
    "pak": {
        "version": ">=0.10"
    }
}
